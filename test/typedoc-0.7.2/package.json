{
  "name": "external-module-name-typedoc-0.7.2",
  "version": "1.0.0",
  "description": "Test against typedoc 0.7.2",
  "main": "index.js",
  "scripts": {
    "build": "typedoc --tsconfig tsconfig.json --out dist",
    "test": "run-s build e2e",
    "e2e": "run-p -r browser-sync cypress",
    "browser-sync": "browser-sync start --port 4000 --server dist --no-open",
    "cypress": "wait-on tcp:4000 && cypress run"
  },
  "author": "Chris Thielen",
  "license": "MIT",
  "dependencies": {
    "browser-sync": "^2.23.6",
    "cypress": "^1.4.2",
    "npm-run-all": "^4.1.2",
    "typedoc": "0.7.2",
    "typedoc-plugin-external-module-name": "latest",
    "wait-on": "^2.1.0"
  }
}
